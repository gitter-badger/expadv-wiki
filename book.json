{
    "styles": {
        "website": "styles/website.css"
    },
    "variables": {
        "repo_name": "Rusketh/ExpAdv2",
        "repo_master_url": "https://github.com/Rusketh/ExpAdv2"
    },
    "plugins": [
        "addcssjs",
        "language-picker",
        "collapsible-menu",
        "anchor-navigation",
        "gist",
        "disqus"
    ],
    "pluginsConfig": {
        "addcssjs": {
            "css": [
                "./assets/css/common.css"
            ],
            "js": [
                "./js/smartTables.js",
                "./js/CGHAPI.js",
                "./js/main.js"
            ]
        },
        "disqus": {
            "shortName": "expadv",
            "useIdentifier": true
        }
    }
}